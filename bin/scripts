#!/usr/bin/env bash
scripts=$(cat <<EOF
clips
configs
generate_cpf
generate_password
launcher
machines
networks
notes
passwords
pomo
power
queries
screenshot
search
tmux_projects
tmux_windows
todos
translate
vim_projects
webstorm_projects
windows
EOF
)
script=$(printf "$scripts" | dmenu -p "script:")
[[ -z "$script" ]] && exit
$script

#!/usr/bin/env bash
scripts=$(cat <<EOF
clips
configs
gcpf
gpassword
launcher
machines
networks
notes
passwords
pomo
power
queries
screenshot
search
todos
tprojects
translate
twindows
vprojects
windows
wprojects
EOF
)
script=$(printf "$scripts" | dmenu -p "script:")
[[ -z "$script" ]] && exit
$script
